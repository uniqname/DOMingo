{
  "name": "DOMingo",
  "version": "3.0.3",
  "description": "A DOM centric rendering engine",
  "main": "DOMingo.js",
  "jsnext:main": "src/DOMingo.js",
  "scripts": {
    "test": "eslint src/*.js; { for f in src/*-test.js; do babel-node $f; done } | tap-spec",
    "build": "rollup -i src/DOMingo.js | babel > DOMingo.js && cat DOMingo.js | uglifyjs -emc > DOMingo.min.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/uniqname/DOMingo.git"
  },
  "keywords": [
    "DOM",
    "templates",
    "templating",
    "rendering",
    "render"
  ],
  "author": "Cory Brown <oh.wise.man@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/uniqname/DOMingo/issues"
  },
  "browserify": {
    "transform": "babel"
  },
  "homepage": "https://github.com/uniqname/DOMingo",
  "devDependencies": {
    "babel": "^5.8.23",
    "babelify": "^6.0.2",
    "browserify": "^9.0.8",
    "jsdom": "^7.0.2",
    "tape": "^4.2.1",
    "uglify-js": "^2.4.20"
  }
}
